
/* Example 1:

Input: s = "Let's take LeetCode contest"
Output: "s'teL ekat edoCteeL tsetnoc" */

/**
 * @param {string} s
 * @return {string}
 */
var reverseWords = function(s) {
   let sSplit = s.split(' ');
    for(let i = 0; i< sSplit.length; i++){
        sSplit[i] = sSplit[i].split('').reverse().join('');
    }
    return sSplit.join(' ');
} ;